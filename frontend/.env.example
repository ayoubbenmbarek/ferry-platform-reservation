# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# API Base URL
REACT_APP_API_URL=http://localhost:8010

# Google OAuth (Get from https://console.cloud.google.com/)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Stripe (Get from https://dashboard.stripe.com/test/apikeys)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# OpenAI (for voice search - optional)
REACT_APP_OPENAI_API_KEY=sk-your-openai-api-key

# Feature Flags
REACT_APP_ENABLE_VOICE_SEARCH=false
REACT_APP_ENABLE_GOOGLE_LOGIN=true
REACT_APP_ENABLE_APPLE_PAY=true
